# Shopify App Configuration
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here
SCOPES=read_products,write_products,read_themes,read_customers

# App URLs (ngrok or production)
HOST=localhost:3000
APP_URL=https://your-ngrok-subdomain.ngrok.app

# Database
DATABASE_URL=file:./prisma/dev.sqlite

# Session encryption keys (comma-separated)
SESSION_KEYS=your,session,keys,here

# Encryption key for access tokens (32 bytes hex)
ENCRYPTION_KEY=abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789